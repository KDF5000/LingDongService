<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.moment.beans.userInfo.SecretMsg" table="secretMsg">
        <id name="msgId" column="id" type="java.lang.Integer">   
        <generator class="native" />  
        </id> 
        <property name="sendUserId" column="sendUserId" type="java.lang.Integer"></property>
        <property name="sendUserAvatar" column="sendUserAvatar" type="java.lang.String"></property>
        <property name="recieveUserId" column="recieveUserId" type="java.lang.Integer"></property>
         <property name="recieveUserAvatar" column="recieveUserAvatar" type="java.lang.String"></property>
        <property name="sendTime" column="sendTime" type="java.lang.String"></property>
        <property name="msgContent" column="content" type="java.lang.String"></property>
        <property name="isRead" column="isRead" type="java.lang.Integer"></property>
        <property name="sendUserName" column="sendUserName" type="java.lang.String"></property>
    </class>
</hibernate-mapping>